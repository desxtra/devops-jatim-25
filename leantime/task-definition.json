{
  "family": "lks-leantime-app",
  "taskRoleArn": "arn:aws:iam::732839262434:role/LabRole",
  "executionRoleArn": "arn:aws:iam::732839262434:role/LabRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "volumes": [
    {
      "efsVolumeConfiguration": {
        "authorizationConfig": {
          "accessPointId": "fsap-0d58b96a6c32a42c1",
          "iam": "ENABLED"
        },
        "fileSystemId": "fs-0549e2504d1251e05",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED"
      },
      "name": "pub-userfiles"
    },
    {
      "efsVolumeConfiguration": {
        "authorizationConfig": {
          "accessPointId": "fsap-0dc0d3258267f8c78",
          "iam": "ENABLED"
        },
        "fileSystemId": "fs-0549e2504d1251e05",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED"
      },
      "name": "userfiles"
    },
    {
      "efsVolumeConfiguration": {
        "authorizationConfig": {
          "accessPointId": "fsap-0455b0b277c95b46a",
          "iam": "ENABLED"
        },
        "fileSystemId": "fs-0549e2504d1251e05",
        "rootDirectory": "/",
        "transitEncryption": "ENABLED"
      },
      "name": "plugins"
    }
  ],
  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false,
  "containerDefinitions": [
    {
      "name": "default",
      "image": "leantime/leantime:latest",
      "cpu": 0,
      "essential": true,
      "volumesFrom": [],
      "systemControls": [],
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "default",
          "protocol": "tcp"
        }
      ],
      "secrets": [],
      "environment": [],
      "mountPoints": [
        {
          "containerPath": "/var/www/html/public/userfiles",
          "readOnly": false,
          "sourceVolume": "pub-userfiles"
        },
        {
          "containerPath": "/var/www/html/userfiles",
          "readOnly": false,
          "sourceVolume": "userfiles"
        },
        {
          "containerPath": "/var/www/html/app/Plugins",
          "readOnly": false,
          "sourceVolume": "plugins"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lks/leantime-app",
          "awslogs-region": "us-east-1",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
