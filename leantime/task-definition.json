{
  "family": "lks-leantime-app",
  "taskRoleArn": "arn:aws:iam::732839262434:role/LabRole",
  "executionRoleArn": "arn:aws:iam::732839262434:role/LabRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "volumes": [],
  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false,
  "containerDefinitions": [
    {
      "name": "default",
      "image": "leantime/leantime:latest",
      "cpu": 0,
      "essential": true,
      "volumesFrom": [],
      "systemControls": [],
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "default",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "MYSQL_HOST",
          "valueFrom": "arn:aws:ssm:us-east-1:732839262434:parameter/lks/db/host"
        },
        {
          "name": "MYSQL_USER",
          "valueFrom": "arn:aws:ssm:us-east-1:732839262434:parameter/lks/db/username"
        },
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "valueFrom": "arn:aws:ssm:us-east-1:732839262434:parameter/lks/db/password"
        },
        {
          "name": "MYSQL_DATABASE",
          "valueFrom": "arn:aws:ssm:us-east-1:732839262434:parameter/lks/db/name"
        },
        {
          "name": "MYSQL_PORT",
          "valueFrom": "arn:aws:ssm:us-east-1:732839262434:parameter/lks/db/port"
        }
      ],
      "environment": [
        {
          "name": "LEAN_DEBUG",
          "value": "0"
        },
        {
          "name": "LEAN_PORT",
          "value": "8080"
        }
      ],
      "mountPoints": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lks/leantime-app",
          "awslogs-region": "us-east-1",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}